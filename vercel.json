{
    "version": 2,
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        },
        {
            "src": "./public/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        },
        {
            "src": "/ser.js(.*)",
            "dest": "public/ser.js"
        },
        {
            "src": "/coo.css(.*)",
            "dest": "public/coo.css"
        }
    ],
    "outputDirectory": "public"
}