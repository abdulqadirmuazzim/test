{
    "version": 2,
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        },
        {
            "src": "./public/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        },
        {
            "src": "/public/(.*)",
            "dest": "ser.js"
        },
        {
            "src": "/public/(.*)",
            "dest": "coo.css"
        },
        {
            "src": "/public/(.*)",
            "dest": "home.html"
        }
    ]
}